                                 :order 12)
                          (:name "Projects"
                                 :tag "project"
                                 :order 14)
                          (:name "Emacs"
                                 :tag "Emacs"
                                 :order 13)
                          (:name "In-Progress"
                                 :todo "IN-PROGRESS"
                                 :order 13)
                          (:name "Research"
                                 :tag "Research"
                                 :order 15)
                          (:name "To read"
                                 :tag "read"
                                 :order 30)
                          (:name "To Learn"
                                 :tag "learn"
                                 :order 20)

                          (:order-multi (40 (:name "Done today"
                                                   :and (:regexp "State \"DONE\""
                                                                 :log t))
                                            (:name "Clocked today"
                                                   :log t
                                                   )))
                          (:name "Waiting"
                                 :and(:todo "WAIT" :not(:tag "work"))
